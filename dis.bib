	file = {Andreas Wagner - 2005 - Robustness and Evolvability in Living Systems.pdf:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/WM6UBE8R/Andreas Wagner - 2005 - Robustness and Evolvability in Living Systems.pdf:application/pdf;ebrary Snapshot:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/BZDSKJ5B/docDetail.html:text/html;ebrary Snapshot:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/DXV5A4R3/docDetail.html:text/html;ebrary Snapshot:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/MKZ4NR7S/docDetail.html:text/html;ebrary Snapshot:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/MMM2GRW4/docDetail.html:text/html;MSU Libraries /All Locations:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/4QCGUFEA/record=b4877428~S39a.html:text/html}
