	keywords = {Avida, Biological Evolution, Computer Simulation, Environment, Epistasis, Genetic architecture, Models, Biological, Modularity, Recombination, Genetic, Reproduction, Selection, Genetic, Sex, Software, digital evolution, evolution of sex},
	pages = {S46--S54},
