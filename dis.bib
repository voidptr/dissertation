@article{pavlicev_model_2012,
	title = {A model of developmental evolution: selection, pleiotropy and compensation},
	volume = {27},
	issn = {0169-5347},
	shorttitle = {A model of developmental evolution},
	url = {http://www.sciencedirect.com/science/article/pii/S0169534712000353},
	doi = {10.1016/j.tree.2012.01.016},
	abstract = {Development and physiology translate genetic variation into phenotypic variation and determine the genotype–phenotype map, such as which gene affects which character (pleiotropy). Any genetic change in this mapping reflects a change in development. Here, we discuss evidence for variation in pleiotropy and propose the selection, pleiotropy and compensation model (SPC) for adaptive evolution. It predicts that adaptive change in one character is associated with deleterious pleiotropy in others and subsequent selection to compensate for these pleiotropic effects. The SPC model provides a unifying perspective for a variety of puzzling phenomena, including developmental systems drift and character homogenization. The model suggests that most adaptive signatures detected in genome scans could be the result of compensatory changes, rather than of progressive character adaptations.},
	number = {6},
	urldate = {2015-11-21},
	journal = {Trends in Ecology \& Evolution},
	author = {Pavlicev, Mihaela and Wagner, Günter P.},
	month = jun,
