	file = {PubMed Central Full Text PDF:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/CMNQI6TA/Redfield et al. - 1997 - The Evolution of Bacterial Transformation Sex wit.pdf:application/pdf}
