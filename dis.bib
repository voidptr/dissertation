@article{bhattacharyya_domains_2006,
	title = {Domains, {Motifs}, and {Scaffolds}: {The} {Role} of {Modular} {Interactions} in the {Evolution} and {Wiring} of {Cell} {Signaling} {Circuits}},
	volume = {75},
	shorttitle = {Domains, {Motifs}, and {Scaffolds}},
	url = {http://dx.doi.org/10.1146/annurev.biochem.75.103004.142710},
	doi = {10.1146/annurev.biochem.75.103004.142710},
	abstract = {AbstractLiving cells display complex signal processing behaviors, many of which are mediated by networks of proteins specialized for signal transduction. Here we focus on the question of how the remarkably diverse array of eukaryotic signaling circuits may have evolved. Many of the mechanisms that connect signaling proteins into networks are highly modular: The core catalytic activity of a signaling protein is physically and functionally separable from molecular domains or motifs that determine its linkage to both inputs and outputs. This high degree of modularity may make these systems more evolvable—in principle, novel circuits, and therefore highly innovative regulatory behaviors, can arise from relatively simple genetic events such as recombination, deletion, or insertion. In support of this hypothesis, recent studies show that such modular systems can be exploited to engineer nonnatural signaling proteins and pathways with novel behavior.},
	number = {1},
	urldate = {2015-11-13},
	journal = {Annual Review of Biochemistry},
	author = {Bhattacharyya, Roby P. and Reményi, Attila and Yeh, Brian J. and Lim, Wendell A.},
	year = {2006},
	pmid = {16756506},
	keywords = {Modularity, docking, evolvability, signal transduction, synthetic biology},
	pages = {655--680}
}

