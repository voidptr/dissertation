	urldate = {2017-02-15},
	journal = {Phil. Trans. R. Soc. B},
	author = {Speijer, Dave},
	month = oct,
	year = {2016},
	pmid = {27619694},
	pages = {20150530},
	file = {Full Text PDF:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/ZIXWD45Z/Speijer - 2016 - What can we infer about the origin of sex in early.pdf:application/pdf;Snapshot:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/MIA2ZQ4V/20150530.html:text/html}
}

