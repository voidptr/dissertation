	file = {JSTOR Full Text PDF:/home/rosiec/.zotero/zotero/zpznf8rl.default/zotero/storage/K4EHPXHV/Visser et al. - 2009 - Exploring the Effect of Sex on Empirical Fitness L.pdf:application/pdf}
