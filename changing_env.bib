	pages = {S15--S30},
	file = {JSTOR Full Text PDF:/home/rosiec/.zotero/zotero/pv2oii6p.default/zotero/storage/K4EHPXHV/Visser et al. - 2009 - Exploring the Effect of Sex on Empirical Fitness L.pdf:application/pdf}
}

