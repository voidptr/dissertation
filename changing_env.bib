	keywords = {Algorithm design and analysis, Application software, autocorrelation, correlation methods, Cost function, evolutionary algorithms, facility location, fitness landscape analysis, genetic algorithm, genetic algorithms, Genetic mutations, Helium, heuristics, local search space, memetic algorithms, optimisation, Performance analysis, quadratic assignment problem, search problems, Simulated annealing, Testing, Wiring},
	pages = {337--352},
