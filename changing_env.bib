	keywords = {Avida, Biological Evolution, Computer Simulation, digital evolution, Environment, Epistasis, evolution of sex, Genetic architecture, Models, Biological, Modularity, Recombination, Genetic, Reproduction, Selection, Genetic, Sex, Software},
	pages = {S46--S54},
	file = {Full Text PDF:/home/rosiec/.zotero/zotero/pv2oii6p.default/zotero/storage/SR65QM2W/Misevic et al. - 2010 - Experiments with Digital Organisms on the Origin a.pdf:application/pdf;Snapshot:/home/rosiec/.zotero/zotero/pv2oii6p.default/zotero/storage/96NSBEG7/S46.html:text/html}
