@incollection{hogeweg_toward_2012,
	series = {Advances in {Experimental} {Medicine} and {Biology}},
	title = {Toward a {Theory} of {Multilevel} {Evolution}: {Long}-{Term} {Information} {Integration} {Shapes} the {Mutational} {Landscape} and {Enhances} {Evolvability}},
	copyright = {©2012 Springer Science+Business Media, LLC},
	isbn = {978-1-4614-3566-2 978-1-4614-3567-9},
	shorttitle = {Toward a {Theory} of {Multilevel} {Evolution}},
	url = {http://link.springer.com/chapter/10.1007/978-1-4614-3567-9_10},
	abstract = {Most of evolutionary theory has abstracted away from how information is coded in the genome and how this information is transformed into traits on which selection takes place. While in the earliest stages of biological evolution, in the RNA world, the mapping from the genotype into function was largely predefined by the physical–chemical properties of the evolving entities (RNA replicators, e.g. from sequence to folded structure and catalytic sites), in present-day organisms, the mapping itself is the result of evolution. I will review results of several in silico evolutionary studies which examine the consequences of evolving the genetic coding, and the ways this information is transformed, while adapting to prevailing environments. Such multilevel evolution leads to long-term information integration. Through genome, network, and dynamical structuring, the occurrence and/or effect of random mutations becomes nonrandom, and facilitates rapid adaptation. This is what does happen in the in silico experiments. Is it also what did happen in biological evolution? I will discuss some data that suggest that it did. In any case, these results provide us with novel search images to tackle the wealth of biological data.},
	language = {en},
	number = {751},
	urldate = {2015-07-29},
	booktitle = {Evolutionary {Systems} {Biology}},
	publisher = {Springer New York},
	author = {Hogeweg, Paulien},
	editor = {Soyer, Orkun S.},
	year = {2012},
	keywords = {Biomedicine general, Evolutionary Biology, systems biology},
	pages = {195--224},
	file = {Full Text PDF:/home/rosiec/.zotero/zotero/pv2oii6p.default/zotero/storage/9G8ZGBQJ/Hogeweg - 2012 - Toward a Theory of Multilevel Evolution Long-Term.pdf:application/pdf;Snapshot:/home/rosiec/.zotero/zotero/pv2oii6p.default/zotero/storage/U22EU2DM/978-1-4614-3567-9_10.html:text/html}
}

@article{gunter_p._wagner_pleiotropic_2011,
	title = {The pleiotropic structure of the genotype–phenotype map: the evolvability of complex organisms},
	volume = {12},
	copyright = {© 2011 Nature Publishing Group},
	issn = {1471-0056},
	shorttitle = {The pleiotropic structure of the genotype–phenotype map},
	url = {http://www.nature.com.proxy2.cl.msu.edu/nrg/journal/v12/n3/full/nrg2949.html},
	doi = {10.1038/nrg2949},
	abstract = {It was first noticed 100 years ago that mutations tend to affect more than one phenotypic characteristic, a phenomenon that was called 'pleiotropy'. Because pleiotropy was found so frequently, the notion arose that pleiotropy is 'universal'. However, quantitative estimates of pleiotropy have not been available until recently. These estimates show that pleiotropy is highly restricted and are more in line with the notion of variational modularity than with universal pleiotropy. This finding has major implications for the evolvability of complex organisms and the mapping of disease-causing mutations.},
